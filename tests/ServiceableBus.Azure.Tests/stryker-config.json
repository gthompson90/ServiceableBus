{
  "stryker-config": {
    "project": "ServiceableBus.Azure.csproj",
    "report-file-name": "ServiceableBus.Azure",
    "reporters": [
      "html",
      "cleartext"
    ],
    "ignore-methods": [
      "*Log*",
      "*NotImplementedException*"
    ],
    "mutate": [
      "!*/ServiceableBusExtensions.cs"
    ], 
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0}
  }
}
